{
 "snippets.ultisnips.directories": [
  "UltiSnips" 
 ],
 "snippets.userSnippetsDirectory": "~/.config/nvim/UltiSnips",
 "suggest.enablePreselect": false,
 "suggest.noselect": false,
  "coc.source.emoji.filetypes": [
    "markdown",
    "gitcommit",
    "txt",
    "javascript"
  ],
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "trace.server": "verbose",
      "filetypes": [
        "go"
      ]
    }
  }
}


